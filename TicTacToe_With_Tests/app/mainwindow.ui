   <string notr="true">
/* Modern Dark Theme for Tic-Tac-Toe
---------------------------------
Main Colors:
- Background: #2c3e50 (Midnight Blue)
- Foreground/Containers: #34495e (Wet Asphalt)
- Primary Text: #ecf0f1 (Clouds)
- Primary Action: #1abc9c (Turquoise)
- Primary Hover: #16a085 (Green Sea)
- Player X: #3498db (Peter River Blue)
- Player O: #e74c3c (Alizarin Red)
- Input Fields: #2c3e50
*/

QWidget#MainWindow {
    background-color: #2c3e50;
}

QStackedWidget {
    background-color: transparent;
}

/* === LABELS === */
QLabel {
    color: #ecf0f1;
    font-family: &quot;Segoe UI&quot;, sans-serif;
}

QLabel#gameNameLabelPage0,
QLabel#gameNameLabelPage1,
QLabel#gameNameLabelPage_2,
QLabel#gameNameLabelPage_3,
QLabel#gameNameLabelPage_5,
QLabel#gameNameLabelPage_6,
QLabel#gameNameLabelPage_7 {
    color: #ecf0f1;
    font: bold 42pt &quot;Segoe UI&quot;;
}

QLabel#gameStatusLabel {
    color: #ecf0f1;
    font: bold 16pt &quot;Segoe UI&quot;;
    background-color: #34495e;
    border-radius: 8px;
    padding: 10px;
}


/* === BUTTONS === */
QPushButton {
    color: white;
    background-color: #3498db; /* Blue for secondary buttons */
    border: none;
    border-radius: 8px;
    padding: 10px;
    font: bold 12pt &quot;Segoe UI&quot;;
}

QPushButton:hover {
    background-color: #2980b9;
}

/* Primary Action Buttons (Login, Register, etc.) */
QPushButton#loginButton,
QPushButton#registerButton,
QPushButton#playerVsAiButton,
QPushButton#playerVsPlayerButton {
    background-color: #1abc9c; /* Turquoise */
    font-size: 14pt;
}

QPushButton#loginButton:hover,
QPushButton#registerButton:hover,
QPushButton#playerVsAiButton:hover,
QPushButton#playerVsPlayerButton:hover {
    background-color: #16a085;
}

/* Link-style Buttons */
QPushButton#signupLoginPageButton, QPushButton#noWorriesButton, QPushButton#loginButtonSignupPage {
    background-color: transparent;
    color: #1abc9c;
    font-size: 10pt;
    text-decoration: underline;
}


/* === GAME BOARD BUTTONS === */
QGroupBox#groupBox_3 QPushButton {
    font: bold 60pt &quot;Segoe UI&quot;; /* Larger font for X and O */
    background-color: #34495e;
}
QGroupBox#groupBox_3 QPushButton:hover {
    background-color: #4a6278;
}

/* === INPUT FIELDS === */
QLineEdit {
    background-color: #2c3e50;
    color: #ecf0f1;
    border: 1px solid #34495e;
    border-radius: 8px;
    padding: 10px;
    font-size: 11pt;
}

QLineEdit:focus {
    border: 1px solid #1abc9c;
}

/* === GROUP BOXES === */
QGroupBox {
    color: #ecf0f1;
    font: italic 14pt &quot;Segoe UI&quot;;
    border: 1px solid #34495e;
    border-radius: 8px;
    margin-top: 20px;
}

QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top center;
    padding: 5px 10px;
    background-color: #34495e;
    border-radius: 8px;
}

/* === OTHER WIDGETS === */
QCheckBox {
    color: #ecf0f1;
    font-size: 10pt;
}
QRadioButton {
    color: #ecf0f1;
    font-size: 10pt;
}

QListWidget {
    background-color: #34495e;
    border: none;
    border-radius: 8px;
    color: #ecf0f1;
    font-size: 12pt;
}
