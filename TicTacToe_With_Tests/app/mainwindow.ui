/*
 * Professional Catppuccin-Macchiato Dark Theme V4
 */

/* === MAIN WINDOW === */
QWidget#MainWindow {
    background-color: #24273a; /* base */
}

QStackedWidget {
    background-color: transparent;
}


/* === LABELS === */
QLabel {
    color: #cad3f5; /* text */
    font-family: &quot;Segoe UI&quot;, sans-serif;
}

QLabel#gameNameLabelPage0,
QLabel#gameNameLabelPage1,
QLabel#gameNameLabelPage_2,
QLabel#gameNameLabelPage_3,
QLabel#gameNameLabelPage_5,
QLabel#gameNameLabelPage_6,
QLabel#myAccountTitleLabel,
QLabel#aiDifficultyLabel {
    color: #cad3f5; /* text */
    font: bold 40pt &quot;Segoe UI Semibold&quot;;
    padding-bottom: 10px;
}

QLabel#gameStatusLabel {
    color: #24273a; /* base */
    font: bold 16pt &quot;Segoe UI&quot;;
    background-color: #89b4fa; /* blue */
    border-radius: 8px;
    padding: 12px;
}

QLabel#signupLoginPageLabel,
QLabel#existingAccountLabel {
    color: #a5adce; /* subtext0 */
}

QGroupBox#groupBox_7 QLabel {
    font-size: 14pt;
    padding: 5px;
}


/* === BUTTONS === */
QPushButton {
    color: #24273a; /* base */
    background-color: #89b4fa; /* blue */
    border: 1px solid #89b4fa;
    border-radius: 8px;
    padding: 12px;
    font: bold 12pt &quot;Segoe UI&quot;;
    outline: none; /* remove focus rectangle */
}

QPushButton:hover {
    background-color: #a6e3a1; /* green */
    border-color: #a6e3a1;
}

QPushButton#myGameHistoryButton,
QPushButton#myAccountButton,
QPushButton#backButtonGamePage,
QPushButton#resetGameboardButton,
QPushButton#backButtonAccount,
QPushButton#logoutButtonAccount,
QPushButton#backButtonHistory,
QPushButton#replayGameButton,
QPushButton#deleteGameButton,
QPushButton#backButtonReset,
QPushButton#resetPasswordButton,
QPushButton#backButtonAiPage {
    background-color: #5b6078; /* surface2 */
    border-color: #5b6078;
    color: #cad3f5; /* text */
}

QPushButton#myGameHistoryButton:hover,
QPushButton#myAccountButton:hover,
QPushButton#backButtonGamePage:hover,
QPushButton#resetGameboardButton:hover,
QPushButton#backButtonAccount:hover,
QPushButton#logoutButtonAccount:hover,
QPushButton#backButtonHistory:hover,
QPushButton#replayGameButton:hover,
QPushButton#deleteGameButton:hover,
QPushButton#backButtonReset:hover,
QPushButton#resetPasswordButton:hover,
QPushButton#backButtonAiPage:hover {
    background-color: #494d64; /* surface1 */
    border-color: #494d64;
}

QPushButton#signupLoginPageButton,
QPushButton#noWorriesButton,
QPushButton#loginButtonSignupPage,
QPushButton#showLoginPasswordButton,
QPushButton#showSignupPasswordButton,
QPushButton#showResetPasswordButton {
    background-color: transparent;
    color: #89b4fa; /* blue */
    font-size: 10pt;
    font-weight: bold;
    border: none;
    padding: 5px;
}
QPushButton#signupLoginPageButton:hover,
QPushButton#noWorriesButton:hover,
QPushButton#loginButtonSignupPage:hover,
QPushButton#showLoginPasswordButton:hover,
QPushButton#showSignupPasswordButton:hover,
QPushButton#showResetPasswordButton:hover {
    text-decoration: underline;
    color: #a6e3a1; /* green */
}

QPushButton#noWorriesButton { text-align: left; }
QPushButton#showLoginPasswordButton { text-align: right; }
QPushButton#showSignupPasswordButton { text-align: right; }
QPushButton#showResetPasswordButton { text-align: right; }

QPushButton#changePasswordButton {
    background-color: transparent;
    color: #89b4fa; /* blue */
    border: 1px solid #89b4fa;
}
QPushButton#changePasswordButton:hover {
    background-color: #89b4fa;
    color: #24273a;
}

QGroupBox#groupBox_3 QPushButton {
    font: bold 60pt &quot;Segoe UI&quot;;
    background-color: #494d64; /* surface1 */
    color: #cad3f5; /* text */
    border-radius: 12px;
}
QGroupBox#groupBox_3 QPushButton:hover {
    background-color: #5b6078; /* surface2 */
}

/* === INPUT FIELDS === */
QLineEdit {
    background-color: #363a4f; /* surface0 */
    color: #cad3f5; /* text */
    border: 1px solid #494d64; /* surface1 */
    border-radius: 8px;
    padding: 12px;
    font-size: 11pt;
}

QLineEdit:focus {
    border: 1px solid #89b4fa; /* blue */
    background-color: #494d64; /* surface1 */
}

/* === GROUP BOXES (Cards) === */
QGroupBox {
    font: bold 14pt &quot;Segoe UI&quot;;
    background-color: #363a4f; /* surface0 */
    border: 1px solid #494d64; /* surface1 */
    border-radius: 12px;
    margin-top: 15px;
    padding: 15px;
    padding-top: 35px;
}

QGroupBox::title {
    subcontrol-origin: padding;
    subcontrol-position: top center;
    padding: 5px 15px;
    color: #cad3f5;
}

/* === OTHER WIDGETS === */
QRadioButton, QCheckBox {
    color: #cad3f5;
    font-size: 10pt;
    padding: 5px;
}
QRadioButton::indicator, QCheckBox::indicator {
    width: 15px;
    height: 15px;
    border-radius: 4px;
    border: 1px solid #89b4fa;
    background-color: #363a4f;
}
QCheckBox::indicator:checked {
    background-color: #89b4fa;
    image: url(:/i18n/check.svg); /* You need to add a checkmark icon to your resources for this to work */
}
QRadioButton::indicator:checked {
	background-color: #89b4fa;
}

QListWidget {
    background-color: #363a4f;
    border: 1px solid #494d64;
    border-radius: 8px;
    color: #cad3f5;
    font-size: 12pt;
    padding: 5px;
}

QListWidget::item:hover {
    background-color: #494d64;
}
QListWidget::item:selected {
    background-color: #89b4fa;
    color: #24273a;
}

QMessageBox {
    background-color: #363a4f;
}
QMessageBox QLabel {
    color: #cad3f5;
    font-size: 11pt;
}
QMessageBox QPushButton {
    background-color: #89b4fa;
    color: #24273a;
    border-radius: 5px;
    padding: 8px;
    min-width: 70px;
    font-size: 10pt;
}
QMessageBox QPushButton:hover {
    background-color: #a6e3a1;
}
